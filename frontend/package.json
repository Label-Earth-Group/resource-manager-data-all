    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.7",
    "typescript": "5.2.2",
    "watch": "^1.0.2"
  },
  "eslintConfig": {
    "plugins": [
      "prettier",
      "import"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended",
      "plugin:import/recommended"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "authentication",
              "./src/authentication"
            ],
            [
              "reauthentication",
              "./src/reauthentication"
            ],
            [
              "design",
              "./src/design"
            ],
            [
              "globalErrors",
              "./src/globalErrors"
            ],
            [
              "modules",
              "./src/modules"
            ],
            [
              "services",
              "./src/services"
            ],
            [
              "types",
              "./src/types"
            ],
            [
              "utils",
              "./src/utils"
            ],
            [
              "Shared",
              "./src/modules/Shared"
            ]
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx",
            ".json"
          ]
        }
      }
    },
    "rules": {
      "no-unused-vars": "error",
      "no-const-assign": "error",
      "eqeqeq": "error",
      "linebreak-style": "off",
      "no-console": [
        "warn",
        {
          "allow": [
            "error",
            "info"
          ]
        }
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "yarn": "^1.22.19"
  }
}
